<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /
RewriteRule ^viewkron/([a-zA-Z0-9_-]+)/([a-zA-Z0-9_-]+)$ index.php?viewkron=$1
#http://localhost/kronologger2015/view/86
#http://localhost/kronologger2015/?view=86
$RewriteRule ^login$ index.php?ref=register [L]
Options All -Indexes
</IfModule>


